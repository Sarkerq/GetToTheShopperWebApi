<Window x:Class="GetToTheShopper.Clients.Seller.View.ShopMapWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GetToTheShopper.Clients.Seller.View"
        mc:Ignorable="d"
        Title="AddShopMapHelper" Height="400" Width="550" WindowStartupLocation="CenterScreen"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="auto"/>
        </Grid.RowDefinitions>
        <WebBrowser Grid.Row="0" Name="Map" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" LoadCompleted="Map_LoadCompleted"/>
        <Button Grid.Row="1" Click="Button_Click" HorizontalAlignment="Center" Margin="10">
            OK
        </Button>
    </Grid>
</Window>
